                                        <td><label id="updateProxyURL"></label>
                                        <!--<input id="updateProxyURL" type="text" placeholder="Proxy URL" class="textBox"> --></td>
                                    </tr>

                                    <tr>
                                        <td>Target URL:</td>

                                        <td><input class="textBox" id=
                                        "updateProxyTargetURL" placeholder=
                                        "Target URL" type="text"></td>
                                    </tr>

                                    <tr>
                                        <td>Latency:</td>

                                        <td><input class="textBox" id=
                                        "updateProxyLatency" placeholder=
                                        "Latency" type="text"></td>
                                    </tr>
                                    
                                   <tr>
                                        <td>Enable HTTPS?</td>
                                        <td><input id="updateInputHttpsFlag" type="checkbox" name="httpsFlag"></td>
                                    </tr>
                                </table>
                            </fieldset>
                       
                            <br>
                            </br>
                    <div class="modal-footer">
                    <fieldset>
                        <button class="btn btn-warning navbar-btn" id="btnUpdateProxy" type="button">UPDATE PROXY</button>
