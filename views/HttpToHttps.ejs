                    <div class="row">
                        <!-- PROXY LIST-->

                        <div id="proxyList">
                            <h2>Secure Proxy List</h2>

                            <table border="1" class="table table border">
                                <thead>
                                    <tr>
                                        <th>Configuaration Id</th>

                                        <th>TargetURL</th>

                                        <th>Latency</th>

                                        <th>ProxyURL</th>

