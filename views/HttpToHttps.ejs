                 <div id="proxyInfo">
                        <div id="updateProxy">
                            <fieldset>
                                <table>
                                    <tr>
                                        <td><strong>Proxy ID:</strong></td>

                                        <td><span id="proxyID"></span></td>
                                    </tr>

                                    <tr>
